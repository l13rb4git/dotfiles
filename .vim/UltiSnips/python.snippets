snippet doc "Intialize a Doc string"
"""`!v expand("%:p:h:t")`	`!v expand("%:r")`

$1

"""
endsnippet

snippet gn "Get Numero"
${1:numero} = ${2:int}(input("${3:Digite }$4"))
endsnippet

snippet printf "Formmated print for python"
print("$1".format(${2:Args}))
endsnippet

snippet cpf "C formmated print for python"
print("${1:%s}\n" % ${2:Args})
endsnippet
