#include <stdio.h>

void info_programma(){
    printf("Informe 5 numeros.");
}

int maior(int numero, int outro_numero) {
    if (numero >= outro_numero) {
        return numero;
    }
    
    return outro_numero;
}

int menor(int numero, int outro_numero) {
    if (numero <= outro_numero) {
        return numero;
    }
    
    return outro_numero;
}

int maior_numero(int num1, int num2,int num3, int num4, int num5){
    int maior_num;

    maior_num = maior(num1, num2);
    maior_num = maior(maior_num, num3);
    maior_num = maior(maior_num, num4);
    maior_num = maior(maior_num, num5);

    return maior_num;
}

int menor_numero(int num1, int num2,int num3, int num4, int num5){
    int menor_num;

    menor_num = menor(num1, num2);
    menor_num = menor(menor_num, num3);
    menor_num = menor(menor_num, num4);
    menor_num = menor(menor_num, num5);

    return menor_num;
}

int main(){
    info_programma();

    int num1, num2, num3,num4,num5;

    printf("Digite 1 número: ");
    scanf("%d", &num1);

    printf("Digite 2 número: ");
    scanf("%d", &num2);

    printf("Digite 3 número: ");
    scanf("%d", &num3);

    printf("Digite 4 número: ");
    scanf("%d", &num4);

    printf("Digite 5 número: ");
    scanf("%d", &num5);

    int maior_num = maior_numero(num1, num2,num3, num4, num5);
    int menor_num = menor_numero(num1, num2,num3, num4, num5);

    if (maior_num == menor_numero) {
        printf("Numeros iguais.");
    } 
    else {
        printf("Numero maior é %d e %d o menor.", maior_num, menor_num);
    }

    return 0;
}
