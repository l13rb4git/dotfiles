def maior(numero, outro_numero):
    if numero > outro_numero:
        return numero
    return outro_numero


def menor(numero, outro_numero):
    if numero <= outro_numero:
        return numero
    return outro_numero


def maior_numero(num1, num2, num3, num4, num5):
    # Se repete de acordo com a quantidade de números
    maior_num = maior(num1, num2)
    maior_num = maior(maior_num, num3)
    maior_num = maior(maior_num, num4)
    maior_num = maior(maior_num, num5)
    return maior_num


def menor_numero(num1, num2, num3, num4, num5):
    # Se repete de acordo com a quantidade de números
    menor_num = menor(num1, num2)
    menor_num = menor(menor_num, num3)
    menor_num = menor(menor_num, num4)
    menor_num = menor(menor_num, num5)
    return menor_num


if '__main__' == __name__:
    num1 = int(input(">> "))
    num2 = int(input(">> "))
    num3 = int(input(">> "))
    num4 = int(input(">> "))
    num5 = int(input(">> "))
    print(maior_numero(num1, num2, num3, num4, num5))
    print(maior_numero(num1, num2, num3, num4, num5))
