import sys
from PyQt5 import QtWidgets as QtGui


class Window(QtGui.QMainWindow):
    def __init__(self):
        super(Window, self).__init__()
        self.setGeometry(50, 50 , 500, 500)
        self.setWindowTitle("Titulo da Janela!")
        self.show()


app = QtGui.QApplication([])
GUI = Window()
sys.exit(app.exec_())
